    <!-- ~~~~~~ Display ~~~~~~~~ -->
    
     <atc:display atc:idx='displaygoal'>
    <atc:text>Your goal for the previous trial was...</atc:text>
      <atc:link atc:global='goal' />
      <atc:countdown atc:time='5' />
    </atc:display>
   
