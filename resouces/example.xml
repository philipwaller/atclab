  <atc:data>

    <!--  Experimental Parameters  -->

    <atc:param atc:idx='default'>
        
      <atc:update_rate>10000</atc:update_rate>
      <atc:scenario_tester>10</atc:scenario_tester>

      <atc:horizontal_doms>5</atc:horizontal_doms>
      <atc:vertical_doms>999</atc:vertical_doms>

	<atc:colours>
       <atc:conflict_colour atc:blink="true">yellow</atc:conflict_colour>
       <atc:cs_none_colour>black</atc:cs_none_colour>
       <atc:cs_annonced_colour>blue</atc:cs_annonced_colour>
       <atc:cs_proposed_colour atc:blink="true">#FF8000</atc:cs_proposed_colour>
       <atc:cs_accepted_colour>darkgreen</atc:cs_accepted_colour>
       <atc:cs_handoff_colour>darkgreen</atc:cs_handoff_colour>
       <atc:cs_overout_colour>darkgreen</atc:cs_overout_colour>
       <atc:cs_nomore_colour>black</atc:cs_nomore_colour>
       
       
     </atc:colours>
      
       
       <atc:info_block atc:position="TOPRIGHT" >
        <atc:time />
        <atc:goal />
        <atc:score />
        <atc:tool />
      </atc:info_block>
 
      <atc:select_tools>
        <atc:scale atc:position="BOTTOMRIGHT" atc:fixed='true'/>
        <atc:altitude atc:x='20' atc:y='300'  />
        <atc:airspeed />
      </atc:select_tools>

      <atc:keyed_tools>
<!-- 
-->
       <atc:history_tool atc:key="H" atc:tag="History" />
        <atc:srprobe_tool atc:key="P" atc:tag="SR-Probe" />
        <atc:route_tool   atc:key="R" atc:tag="Route" />
        <atc:vector_tool  atc:key="V" atc:tag="Vector" />
	<atc:brline_tool  atc:key="B" atc:tag="BR-Line" />
        <atc:accept_tool  atc:key="A" atc:tag="ACCEPT" />
       <atc:handoff_tool atc:key="N" atc:tag="HANDOFF" /> 
        <atc:pause_tool   atc:key="S" />
        <atc:resume_tool  atc:key="Y" />
      </atc:keyed_tools>
 
 <!--<atc:initial_tools>
      <atc:srprobe_tool atc:toggle="3" />
    </atc:initial_tools>-->
 
     <atc:scoring>
        <atc:initial            atc:value='0'                                             />
     <atc:missed_acceptance  atc:value='10'	atc:audio='failedtoaccept.wav'      atc:duration='2750'  /> 
    </atc:scoring>
    
    
    
    </atc:param>

