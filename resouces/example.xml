<!--
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Reusable UI components
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
-->    
   <!--  ~~~~~ Validated Question (age) ~~~~~  -->

    <atc:validated_question atc:idx='age'>
      <atc:text>How old are you?</atc:text>
      <atc:int_validator atc:min='16' atc:max='90'>
        <atc:text>* Please enter a value between '16' and '90' inclusive!</atc:text>
      </atc:int_validator>
    </atc:validated_question>

    <!--  ~~~~~ Choice Question (gender)  ~~~~~  -->

    <atc:choice_question atc:idx='gender'>
      <atc:text>What is your gender?</atc:text>
      <atc:choice atc:value='0'>Male</atc:choice>
      <atc:choice atc:value='1'>Female</atc:choice>
    </atc:choice_question>
    
  
    <!--  ~~~~~ Scale Question ~~~~~  -->

    <atc:scale_question atc:idx='genericscale'>
      <atc:text>The question appears here...</atc:text>
      <atc:label>Low</atc:label>
      <atc:label>Moderate</atc:label>
      <atc:label>High</atc:label>
      <atc:scale atc:min='0' atc:max='10' />
    </atc:scale_question>


<!--  ~~~~~ Multi Question ~~~~~  -->

<atc:multi_question atc:idx='genericmulti'>
      <atc:text>This text appears above the series of questions.</atc:text>
      <atc:label>Low</atc:label>
      <atc:label>Moderate</atc:label>
      <atc:label>High</atc:label>
      <atc:scale atc:min='1' atc:max='5' />
	<atc:question> Question 1...</atc:question>
	<atc:question> Question 2...</atc:question>
	<atc:question> Question 3...</atc:question>
	<atc:question> Question 4...</atc:question>
	<atc:question> Question 5...</atc:question>
	<atc:question> Question 6...</atc:question>
	<atc:question> Question 7...</atc:question>
	<atc:question> Question 8...</atc:question>    
	<atc:question> Question 9...</atc:question>
	<atc:question> Question 10...</atc:question>
     <atc:number />
     <atc:answer_all /> 
    </atc:multi_question>

     <!--  ~~~~~ Personal Goal Question ~~~~~  -->
    
     <atc:validated_question atc:idx='personal_goal'>
      <atc:text>What is your goal for the next trial?</atc:text>
      <atc:int_validator atc:min='0' atc:max='730'>
        <atc:text>* Please enter a value between '0' and '730' inclusive!</atc:text>
      </atc:int_validator>
      <atc:link atc:global='goal'/>
   </atc:validated_question>

     <!--  ~~~~~ Within-trial Questions ~~~~~  -->
    
    <atc:scale_question atc:idx='within_trial1'>
      <atc:text>The question appears here...</atc:text>
      <atc:label>Low</atc:label>
      <atc:label>Moderate</atc:label>
      <atc:label>High</atc:label>
      <atc:scale atc:min='0' atc:max='10' />
    </atc:scale_question>
    
      <atc:scale_question atc:idx='within_trial2'>
      <atc:text>The question appears here...</atc:text>
      <atc:label>Strongly Disagree</atc:label>
      <atc:label>Disagree</atc:label>
      <atc:label>Neutral</atc:label>
      <atc:label>Agree</atc:label>
      <atc:label>Strongly Agree</atc:label>
      <atc:scale atc:min='1' atc:max='7' />
    </atc:scale_question>

  <atc:scale_question atc:idx='within_trial3'>
