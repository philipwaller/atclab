      <atc:label>Very Unlikely</atc:label>
      <atc:label>Unlikely</atc:label>
      <atc:label>Likely</atc:label>
      <atc:label>Very Likely</atc:label>
      <atc:scale atc:min='1' atc:max='4' />
    </atc:scale_question>



